<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>action</servlet-name>
        <servlet-class>org.apache.struts.action.ActionServlet</servlet-class>
        <init-param>
            <param-name>config</param-name>
            <param-value>/WEB-INF/struts-config.xml</param-value>
        </init-param>
        <init-param>
            <param-name>debug</param-name>
            <param-value>2</param-value>
        </init-param>
        <init-param>
            <param-name>detail</param-name>
            <param-value>2</param-value>
        </init-param>
        <load-on-startup>2</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>AjaxServlet</servlet-name>
        <servlet-class>com.classifiedindia.servlet.AjaxServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadServlet</servlet-name>
        <servlet-class>com.classifiedindia.servlet.UploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MobileBrandAjaxServlet</servlet-name>
        <servlet-class>com.classifiedindia.servlet.MobileBrandAjaxServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccesoryServlet</servlet-name>
        <servlet-class>com.classifiedindia.servlet.AccesoryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccesoryBrandServlet</servlet-name>
        <servlet-class>com.classifiedindia.servlet.AccesoryBrandServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MobiSimNoServlet</servlet-name>
        <servlet-class>com.classifiedindia.servlet.MobiSimNoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MobiOSServlet</servlet-name>
        <servlet-class>com.classifiedindia.servlet.MobiOSServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TabletServlet</servlet-name>
        <servlet-class>com.classifiedindia.servlet.TabletServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.classifiedindia.servlet.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>action</servlet-name>
        <url-pattern>*.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AjaxServlet</servlet-name>
        <url-pattern>/AjaxServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadServlet</servlet-name>
        <url-pattern>/UploadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MobileBrandAjaxServlet</servlet-name>
        <url-pattern>/MobileBrandAjaxServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccesoryServlet</servlet-name>
        <url-pattern>/AccesoryServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccesoryBrandServlet</servlet-name>
        <url-pattern>/AccesoryBrandServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MobiSimNoServlet</servlet-name>
        <url-pattern>/MobiSimNoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MobiOSServlet</servlet-name>
        <url-pattern>/MobiOSServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TabletServlet</servlet-name>
        <url-pattern>/TabletServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <jsp-config>
        <taglib>
            <taglib-uri>/WEB-INF/struts-bean.tld</taglib-uri>
            <taglib-location>/WEB-INF/struts-bean.tld</taglib-location>
        </taglib>
        <taglib>
            <taglib-uri>/WEB-INF/struts-html.tld</taglib-uri>
            <taglib-location>/WEB-INF/struts-html.tld</taglib-location>
        </taglib>
        <taglib>
            <taglib-uri>/WEB-INF/struts-logic.tld</taglib-uri>
            <taglib-location>/WEB-INF/struts-logic.tld</taglib-location>
        </taglib>
        <taglib>
            <taglib-uri>/WEB-INF/struts-nested.tld</taglib-uri>
            <taglib-location>/WEB-INF/struts-nested.tld</taglib-location>
        </taglib>
        <taglib>
            <taglib-uri>/WEB-INF/struts-tiles.tld</taglib-uri>
            <taglib-location>/WEB-INF/struts-tiles.tld</taglib-location>
        </taglib>
    </jsp-config>
    <resource-ref>
        <res-ref-name>jndi</res-ref-name>
        <res-type>javax.mail.Session</res-type>
        <res-auth>Container</res-auth>
        <res-sharing-scope>Shareable</res-sharing-scope>
    </resource-ref>
</web-app>
